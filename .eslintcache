er: yup\r\n      .string()\r\n      .test('len', 'Fill all space', val => {\r\n        const val_length_without_dashes = val.replace(/\\D/g, '').length;\r\n        return val_length_without_dashes === 11;\r\n      })\r\n      .required(),\r\n    email: yup.string().email().required(),\r\n  });\r\n\r\n  //  Reaact hook Form\r\n  // ========================================\r\n  const { register, errors, handleSubmit } = useForm({\r\n    resolver: yupResolver(schema),\r\n    defaultValues: defaultValues(), //  for reset form with default values\r\n  });\r\n\r\n  // react hook form with default values part №1\r\n  // ====================================\r\n  function defaultValues() {\r\n    if (contactObj) {\r\n      const { firstName, lastName, phoneNumber, email } = contactObj;\r\n      return {\r\n        firstName,\r\n        lastName,\r\n        phoneNumber,\r\n        email,\r\n      };\r\n    }\r\n    return {\r\n      firstName: '',\r\n      lastName: '',\r\n      phoneNumber: '',\r\n      email: '',\r\n    };\r\n  }\r\n\r\n  // Submit Form\r\n  // ==============================================================\r\n  const onSubmit = (data, e) => {\r\n    const newContact = {\r\n      name: data.firstName,\r\n      number: data.phoneNumber,\r\n    };\r\n\r\n    dispatch(addContact(newContact));\r\n    e.target.reset();\r\n    onClose();\r\n    return;\r\n\r\n    //  добавить изменение контактов\r\n  };\r\n\r\n  return (\r\n    <div className={s.formWrapper}>\r\n      <form className={s.form} onSubmit={handleSubmit(onSubmit)}>\r\n        <h2 className={s.title}>\r\n          {contactObj ? 'Change Contact' : 'New Contact'}\r\n        </h2>\r\n\r\n        <label className={s.label}>\r\n          <span className={s.inputTitle}>First Name</span>\r\n          <input\r\n            type=\"name\"\r\n            name=\"firstName\"\r\n            className={s.input}\r\n            ref={register({ required: true, maxLength: 20 })}\r\n          />\r\n          <p className={s.error}>{errors.firstName?.message}</p>\r\n        </label>\r\n\r\n        <label className={s.label}>\r\n          <span className={s.inputTitle}>Second Name</span>\r\n          <input\r\n            type=\"name\"\r\n            name=\"lastName\"\r\n            className={s.input}\r\n            ref={register({ required: true, maxLength: 20 })}\r\n          />\r\n          <p className={s.error}>{errors.lastName?.message}</p>\r\n        </label>\r\n\r\n        <label className={s.label}>\r\n          <span className={s.inputTitle}>E-mail</span>\r\n          <input name=\"email\" className={s.input} ref={register} />\r\n          <p className={s.error}>{errors.email?.message}</p>\r\n        </label>\r\n\r\n        <label className={s.label}>\r\n          <span className={s.inputTitle}>Phone number</span>\r\n          <InputMask\r\n            name=\"phoneNumber\"\r\n            mask=\"+3 (999) 999-99-99\"\r\n            alwaysShowMask={true}\r\n            className={s.input}\r\n            ref={register}\r\n          />\r\n          <p className={s.error}>{errors.phoneNumber?.message}</p>\r\n        </label>\r\n\r\n        <label className={s.label}>\r\n          <span className={s.inputTitle} style={{ display: 'block' }}>\r\n            Add to Chosen?\r\n          </span>\r\n          <input type=\"checkbox\" />\r\n        </label>\r\n        {contactObj ? (\r\n          <button className={s.btn} type=\"submit\">\r\n            Save changes\r\n          </button>\r\n        ) : (\r\n          <button type=\"submit\" className={s.btn}>\r\n            Add contact\r\n          </button>\r\n        )}\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Projects\\Homeworks GoIT\\js\\React\\goit-react-hw-05-movies\\phonebook-practice\\src\\App.js",[],"C:\\Projects\\Homeworks GoIT\\js\\React\\goit-react-hw-05-movies\\phonebook-practice\\src\\Components\\Filter\\Filter.jsx",[],"C:\\Projects\\Homeworks GoIT\\js\\React\\goit-react-hw-05-movies\\phonebook-practice\\src\\i